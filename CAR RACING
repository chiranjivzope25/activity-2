#include <stdio.h>
   struct candidate{
   int  thar;
   int gwag;
   int fort;
   int bmw;
   int rang;

};

int main() {
    int i =1;
    int total = 0;
    int th,gw,fo,bm,ra;
    printf("Enter  the your cars you have \n");
     printf("cars taken in Race : \n 1.Thar\n 2.G Wagan\n 3.fortuner\n 4.BMW\n 5.Range Rower");
    for(int i = 1;i<=100;i++){
    printf("enter the number of cars  of %d candidiate\n",i);
    
     printf(" enter the no. of Thars\n");
     scanf("%d",&th);
     printf(" enter the marks of G Wagan\n");
     scanf("%d",&gw);
      printf(" enter the no. of Fortuner\n");
     scanf("%d",&fo);
     printf(" enter the no. of BMW\n");
     scanf("%d",&bm);
    printf(" enter the no. of Range Rower\n");
     scanf("%d",&ra);
     
    struct candidate si={th,gw,fo,bm,ra};
    
     total = si.thar + si.gwag +  si.fort +si.rang;
    if(total>=5 ){
    printf("You can take participate in Race\n");
       printf(" congratulations \nYou have  total  %d cars   ",total);
    }
    
      else{
     printf("You cannot take participate in Race\n");
    printf(" congratulations \nYour total  %d cars   ",total);
    
    }
    
    printf("\n");
    }
    
    return 0;
}

